 @model Technician
 @{
     string title = ViewBag.Action + " Technician";
     ViewBag.Title = title;
 }

 <h1>@ViewBag.Title</h1>

 <form asp-action="Save" method="post">
     <div asp-validation-summary="All" class="text-danger"></div>

    <div class="row mb-3">
        <label asp-for="TechnicianID" class="col-form-label col-md-2">Technician</label>
        <div class="col">
            <input asp-for="TechnicianID" class="form-control" />
        </div>
    </div>

    <div class="row mb-3">
        <label asp-for="Email" class="col-form-label col-md-2">Email</label>
        <div class="col">
            <input asp-for="Email" class="form-control" />
        </div>
    </div>

    <div class="row mb-3">
        <label asp-for="Phone" class="col-form-label col-md-2">Phone</label>
        <div class="col">
            <input asp-for="Phone" class="form-control" />
        </div>
    </div>

     <input type="hidden" asp-for="TechnicianID" />

     <div class="row">
         <div class="col">
             <button type="submit" class="btn btn-primary">Save</button>
             <a asp-action="List" class="btn btn-primary">Cancel</a>
         </div>
     </div>

 </form>

 @section Scripts {
     <partial name="_ValidationScriptsPartial" />
 }