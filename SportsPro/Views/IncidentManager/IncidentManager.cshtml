@model SportsPro.Models.ViewModels.IncidentManagerViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Incident Manager</title>
</head>
<body>
    <h2>@Model.DisplayMode Incidents</h2>

    <ul>
        @foreach (var incident in Model.Incidents)
        {
            <li>@incident.Title</li>
        }
    </ul>
</body>
</html>
